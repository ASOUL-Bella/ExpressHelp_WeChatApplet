              </navigator>
            </view>
          </block>
          <block wx:for='{{ordersList}}' wx:for-item="order" wx:key="orderId" wx:if="{{navbarActiveIndex == 2 & order.orderStatus == '待接取' & order.customerName == userInfo.nickName}}">
